{
  "name": "rune-games-sdk",
  "version": "4.11.0",
  "scripts": {
    "build": "yarn generateStateMachineTypes && rm -rf dist/* && rollup -c --banner \"$(cat copyright.txt)\"",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts",
    "typecheck": "yarn generateStateMachineTypes && tsc --noEmit",
    "format": "prettier --write .",
    "start": "vite dev-ui",
    "pretest": "yarn generateStateMachineTypes",
    "test": "jest && yarn test:browserCompatibility",
    "test:watch": "JEST_WATCH_TESTS=1 jest --watch",
    "test:browserCompatibility": "es-check es3 singleplayer.js multiplayer.js",
    "test:updateSnapshots": "jest -u",
    "release": "scripts/release.sh",
    "generateStateMachineTypes": "xstate typegen 'src/singleplayer/lib/stateMachine.ts' && xstate typegen 'src/multiplayer/libClient/stateMachine.ts'",
    "generateAdapters": "ts-node -P tsconfig.cli.json adapters/generate.ts",
    "decode": "node scripts/decode.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "8.3.3",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^7.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "@xstate/cli": "0.1.7",
    "es-check": "^7.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-only-ascii": "^0.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nanoevents": "^7.0.1",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-json-pretty": "^2.2.0",
    "rollup": "^3.3.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.9",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.3",
    "uuid": "^9.0.0",
    "vite": "^4.3.8",
    "vite-plugin-svgr": "^3.2.0"
  },
  "dependencies": {
    "base-64": "1.0.0",
    "core-js": "3.31.1",
    "immer": "10.0.1",
    "rune-msgpack": "1.0.0",
    "xstate": "4.32.1"
  },
  "files": [
    "dist",
    "singleplayer.js",
    "singleplayer.d.ts",
    "multiplayer.js",
    "multiplayer.d.ts",
    "multiplayer-dev.js"
  ]
}
